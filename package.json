{
  "name": "angular-grid-layout",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:client",
    "build:lib": "ng build drag-drop-layout --configuration production",
    "build:client": "ng build client --configuration production",
    "build:client:github-pages": "ng build client --configuration github-pages",
    "watch": "ng build --watch --configuration development",
    "watch:lib": "ng build drag-drop-layout --watch --configuration development",
    "serve:client": "ng serve client --port 80 --host 0.0.0.0",
    "test": "ng test",
    "serve:ssr:client": "node dist/client/server/server.mjs"
  },
  "dependencies": {
    "@angular/animations": "^18.1.4",
    "@angular/common": "^18.1.4",
    "@angular/compiler": "^18.1.4",
    "@angular/core": "^18.1.4",
    "@angular/forms": "^18.1.4",
    "@angular/platform-browser": "^18.1.4",
    "@angular/platform-browser-dynamic": "^18.1.4",
    "@angular/platform-server": "^18.1.4",
    "@angular/router": "^18.1.4",
    "@angular/ssr": "^18.0.3",
    "express": "^4.18.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.4",
    "@angular/cli": "^18.1.4",
    "@angular/compiler-cli": "^18.1.4",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-coverage": "~2.2.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.0.0",
    "typescript": "~5.4.2"
  }
}
